; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="barkvr"
run/main_scene="res://mainSystem/scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="bark"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 0)
boot_splash/image="res://assets/icons/barkiconrounded.png"
boot_splash/fullsize=false
config/icon="res://assets/icons/barkiconrounded.png"
config/windows_native_icon="res://assets/barkvrcorgi.ico"

[audio]

driver/enable_input=true
driver/mix_rate=48000

[autoload]

LocalGlobals="*res://mainSystem/scripts/localGlobals.gd"
BarkHelpers="*res://mainSystem/scripts/bark_helpers.gd"
Notifyvr="*res://addons/notifyvr/notifyvr.gd"

[bark]

webrtc_config/stun_servers=PackedStringArray("stun:stun.l.google.com:19302", "stun:stun1.l.google.com:19302", "stun:stun2.l.google.com:19302", "stun:stun3.l.google.com:19302", "stun:stun4.l.google.com:19302")
webrtc_config/turn_servers=PackedStringArray()
webrtc_config/turn_password=""
webrtc_config/turn_username=""

[display]

window/size/viewport_width=1500
window/size/viewport_height=800
window/subwindows/embed_subwindows=false

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/Godot-MToon-Shader/plugin.cfg", "res://addons/Panel3D/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/anthonyec.camera_preview/plugin.cfg", "res://addons/contextmenu/plugin.cfg", "res://addons/unidot_importer/plugin.cfg", "res://addons/vrm/plugin.cfg")

[gui]

common/text_edit_undo_stack_max_size=4096
theme/default_theme_scale=1.5
theme/custom="res://assets/barkvr_theme.tres"
theme/default_font_multichannel_signed_distance_field=true
theme/default_font_generate_mipmaps=true

